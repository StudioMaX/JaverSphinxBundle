{
    "name": "pluk77/symfonySphinxBundle",
    "type": "symfony-bundle",
    "description": "Provides integration of Sphinx search engine with Symfony4 using SphinxQL",
    "keywords": ["sphinx", "sphinxql", "search", "symfony", "symfony4", "doctrine2"],
    "homepage": "https://github.com/pluk77",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Borodavko",
            "email": "vadim.borodavko@gmail.com",
            "homepage": "https://github.com/javer"
        },
		{
            "name": "Marcel Berteler",
            "email": "pluk77@gmail.com",
            "homepage": "https://github.com/pluk77"
        }
		
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0,<6.0"
    },
    "suggest": {
        "doctrine/orm": "to allow usage with Doctrine ORM",
    },
    "autoload": {
        "psr-4": {
            "pluk77\\SymfonySphinxBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
